<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Version>1.0.0</Version>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.Http" Version="8.0.0" />
      <PackageReference Include="Refit" Version="7.0.0" />
    </ItemGroup>

    <PropertyGroup>
        <Title>PartyDotNet</Title>
        <Authors>Fawaz Takahji</Authors>
        <Description>A .NET client for the Kemono/Coomer API.</Description>
        <PackageProjectUrl>https://github.com/FawazTakahji/PartyDotNet</PackageProjectUrl>
        <RepositoryUrl>https://github.com/FawazTakahji/PartyDotNet</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>kemono kemono.su kemono.party coomer coomer.su coomer.party</PackageTags>
        <PackageLicenseExpression>GPL-3.0-only</PackageLicenseExpression>
    </PropertyGroup>

</Project>
